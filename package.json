    "prettier": "prettier --write '**/*.{js,json,md}'"
  },
  "dependencies": {
    "itty-router": "^4.0.14",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "query-string": "^8.1.0"
  },
  "devDependencies": {
    "args": "^5.0.3",
    "concat-stream": "^2.0.0",
    "inquirer": "^9.2.8",
    "ngrok": "^4.3.3",
    "prettier": "^3.0.0",
    "toml": "^3.0.0",
    "wrangler": "^3.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
