{
  "name": "chart-img-telegram-bot",
  "version": "1.0.3",
  "description": "chart-img telegram bot",
  "author": "CHART-IMG <developer@chart-img.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npm run ngrok -- --port 8080 -t & npm run server-local -- --ip 127.0.0.1 --port 8080",
